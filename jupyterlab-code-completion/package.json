{
  "name": "jupyterlab-code-completion",
  "version": "0.2.0",
  "description": "A JupyterLab extension for full line code completion",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/isaintnik/full-line-code-completion",
  "bugs": {
    "url": "https://github.com/isaintnik/full-line-code-completion/issues"
  },
  "license": "BSD-3-Clause",
  "author": "*authors here*",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/isaintnik/full-line-code-completion.git"
  },
  "scripts": {
    "build": "tsc",
    "build:labextension": "rimraf lab-dist && mkdirp lab-dist && cd lab-dist && npm pack ..",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/completer": "^1.0.0",
    "@jupyterlab/coreutils": "^3.0.0",
    "@jupyterlab/fileeditor": "^1.0.0",
    "@jupyterlab/notebook": "^1.0.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.1.1"
  },
  "jupyterlab": {
    "extension": true
  }
}
